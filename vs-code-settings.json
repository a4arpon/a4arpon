{
  "extensions.autoUpdate": false,
  "editor.fontLigatures": "'calt', 'ss01', 'ss02'",
  "editor.minimap.enabled": false,
  "workbench.layoutControl.enabled": false,
  "git.enableSmartCommit": true,
  "diffEditor.codeLens": false,
  "extensions.autoCheckUpdates": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.fontSize": 21,
  "editor.lineHeight": 1.6,
  "editor.scrollbar.verticalScrollbarSize": 20,
  "editor.scrollbar.horizontalScrollbarSize": 10,
  "update.mode": "none",
  "editor.wordWrap": "on",
  "editor.fontFamily": "'Cascadia Code','monospace', monospace",
  "window.title": "${rootNameShort}  |  ${dirty} ${activeEditorShort}",
  "editor.cursorBlinking": "solid",
  "telemetry.telemetryLevel": "off",
  "workbench.startupEditor": "none",
  "codesnap.shutterAction": "copy",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.idea": true,
    "**/.yarn/cache": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true
  },
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 18,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "none",
  "editor.dragAndDrop": false,
  "editor.links": false,
  "editor.roundedSelection": true,
  "editor.minimap.renderCharacters": false,
  "workbench.localHistory.enabled": false,
  "workbench.reduceMotion": "on",
  "remote.autoForwardPorts": false,
  "remote.restoreForwardedPorts": false,
  "search.searchOnType": false,
  "terminal.integrated.gpuAcceleration": "on",
  "editor.fontWeight": "500",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "remote.downloadExtensionsLocally": false,
  "remote.forwardOnOpen": false,
  "explorer.confirmDelete": false,
  "workbench.tree.indent": 14,
  "workbench.tree.renderIndentGuides": "always",
  "editor.experimentalWhitespaceRendering": "off",
  "diffEditor.wordWrap": "on",
  "files.participants.timeout": 0,
  "workbench.cloudChanges.autoResume": "off",
  "workbench.cloudChanges.continueOn": "off",
  "workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
  "workbench.commandPalette.history": 5,
  "workbench.editor.empty.hint": "hidden",
  "workbench.sideBar.location": "right",
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "breadcrumbs.enabled": false,
  "window.zoomLevel": 1,
  "accessibility.signals.lineHasInlineSuggestion": {
    "sound": "off"
  },
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.stickyScroll.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "window.commandCenter": false,
  "window.menuBarVisibility": "toggle",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "workbench.activityBar.location": "hidden",
  "workbench.colorCustomizations": {
    "foreground": "#cdcdcd",
    "focusBorder": "#8688ff",
    "selection.background": "#6e71ffb5",
    "scrollbar.shadow": "#ff242400",
    "editor.background": "#0f0f0f",
    "editorCursor.foreground": "#8688ff",
    "sideBar.background": "#0f0f0f",
    "tab.activeBackground": "#0f0f0f",
    "tab.border": "#00000000"
  },
  "supermaven.enableFixWithSupermaven": false,
  "remote.portsAttributes": {
    "4000": {
      "protocol": "https"
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "supermaven.enable": {
    "*": true
  }
}
