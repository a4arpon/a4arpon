{
  "git_panel": {
    "dock": "right",
    "default_width": 480
  },
  "icon_theme": "Zed (Default)",
  "outline_panel": {
    "dock": "right",
    "default_width": 480
  },
  "collaboration_panel": {
    "dock": "right",
    "default_width": 480
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "xR-a4arpon Dark",
  "features": {
    "copilot": false,
    "edit_prediction_provider": "supermaven"
  },
  "project_panel": {
    "dock": "right",
    "default_width": 480,
    "auto_fold_dirs": true,
    "indent_size": 20
  },
  "ui_font_size": 18,
  "ui_font_family": "Comfortaa",
  "buffer_font_size": 22,
  "buffer_font_family": "JetBrains Mono",
  "buffer_line_height": {
    "custom": 1.7
  },
  "autosave": "off",
  "git": {
    "inline_blame": {
      "enabled": false
    },
    "git_gutter": "tracked_files"
  },
  "projects_online_by_default": false,
  "auto_update": false,
  "assistant": {
    "version": "2",
    "enabled": false
  },
  "chat_panel": {
    "button": "never"
  },
  "scrollbar": {
    "show": "always"
  },
  "cursor_blink": false,
  "soft_wrap": "preferred_line_length",
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "always_treat_brackets_as_autoclosed": true,
  "terminal": {
    "font_family": "JetBrains Mono",
    "font_size": 20
  },
  "base_keymap": "VSCode",
  "gutter": {
    "folds": false
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/node_modules",
    "**/dist",
    "**/.next",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.idea",
    "**/.yarn/cache"
  ],
  "show_completion_documentation": false,
  "show_copilot_suggestions": true,
  "show_completions_on_input": true,
  "working_directory": "current_project_directory",
  "show_call_status_icon": false,
  "show_nav_history_buttons": false,
  "node": {
    "path": "/usr/bin/node",
    "npm_path": "/usr/bin/npm",
    "ignore_system_version": false
  },
  "tabs": {
    "file_icons": true,
    "git_status": true,
    "show_close_button": "hidden"
  },
  "use_autoclose": true,
  "cursor_shape": "bar",
  "language_servers": [
    // "vtsls",
    "typescript-language-server",
    "snippet-completion-server",
    "json-language-server",
    "biome",
    "codebook"
  ],
  "lsp": {
    "biome": {
      "settings": {
        "require_config_file": true
      }
    }
  },
  "format_on_save": "on",
  "formatter": {
    "language_server": {
      "name": "biome"
    }
  },
  "code_actions_on_format": {
    "source.fixAll.biome": true,
    "source.removeUnusedImports.ts": true
  }
}
